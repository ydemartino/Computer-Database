<?xml version="1.0" encoding="UTF-8"?>
<configuration>

   <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
       <encoder>
           <Pattern>%d{HH:mm:ss.SSS} [%-5p] %logger{36} | %m %n</Pattern>
       </encoder>
   </appender>

   <root level="info">
       <appender-ref ref="console"/>
   </root>
</configuration>